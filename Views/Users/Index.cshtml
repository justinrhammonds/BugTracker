@model IEnumerable<BugTracker.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>
<table>
    <thead>
        <tr>        
            <th>Last Name</th>
            <th>First Name</th>
            <th>Email</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var user in Model)
        {
            <tr>            
                <td>@user.LastName</td>
                <td>@user.FirstName</td>
                <td>@user.Email</td>
                <td>
                    @Html.ActionLink("Edit", "EditUserRoles","Users", new { id = user.Id }, null)
                </td>
            </tr>
        }
    </tbody>
</table>

